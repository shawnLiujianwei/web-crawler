{
  "mongo": {
    "db": "web_crawler",
    "host": "127.0.0.1",
    "port": 27017
  },
  "nodeLog": {
    "configuration": {
      "appenders": [
        {
          "type": "console"
        }
      ],
      "levels": {
        "[all]": "debug"
      },
      "replaceConsole": true
    }
  },
  "listener": {
    "port": 7001,
    "memoryUsage": 0.90,
    "failureRate": {
      "rate": 0.20,
      "proceed": 200
    },
    "seleniumHub": 4444,
    "phantomCluster": [
      7010,
      7011
    ],
    "seleniumServer": [
      7020
    ]
  }
}